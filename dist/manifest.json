{
    "manifest_version": 2,
    "name": "自由跳转",
    "description": "外链自由跳转",
    "version": "1.0.0",
    "browser_action": {
        "default_icon": "icon-16.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["js/backgroundPage.js"],
        "persistent": false
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://t.cn/*"],
            "js": ["js/contentScript.js"]
        },
        {
            "matches": ["https://www.zhihu.com/*"],
            "js": ["js/zhihu.js"]
        },
        {
            "matches": ["https://www.jianshu.com/*"],
            "js": ["js/jianshu.js"]
        },
        {
            "matches": ["https://weibo.com/*"],
            "js": ["js/weibo.js"]
        }
    ],
    "permissions": ["tabs", "activeTab", "notifications", "http://*/", "https://*/", "webNavigation", "http://api.t.sina.com.cn/*"]
}
